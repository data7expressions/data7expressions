#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.14.8_ts-node@10.9.2_@types+node@20.14.8_typescript@5.4.5_/node_modules/jest-cli/bin/node_modules:/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.14.8_ts-node@10.9.2_@types+node@20.14.8_typescript@5.4.5_/node_modules/jest-cli/node_modules:/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.14.8_ts-node@10.9.2_@types+node@20.14.8_typescript@5.4.5_/node_modules:/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.14.8_ts-node@10.9.2_@types+node@20.14.8_typescript@5.4.5_/node_modules/jest-cli/bin/node_modules:/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.14.8_ts-node@10.9.2_@types+node@20.14.8_typescript@5.4.5_/node_modules/jest-cli/node_modules:/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.14.8_ts-node@10.9.2_@types+node@20.14.8_typescript@5.4.5_/node_modules:/home/flavio/personal/org/data7expressions/data7expressions/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
